pany, setCompany] = React.useState('');\r\n    const [error, setError] = React.useState(false);\r\n    const navigate = useNavigate();\r\n    const params = useParams();\r\n\r\n    useEffect(() => {\r\n        getProductDetails();\r\n    },[]);\r\n\r\n    const getProductDetails = () => {\r\n        Axios.get(`http://localhost:5000/api/update/${params.id}`).then((response) => {\r\n            //console.log(response.data);\r\n            setProdName(response.data[0].name);\r\n            setPrice(response.data[0].price);\r\n            setCompany(response.data[0].brand);\r\n            setCategory(response.data[0].category);\r\n        });\r\n    }\r\n\r\n    const updateProduct = (id)=> {\r\n        if (!prodName || !price || !company || !category)\r\n        {\r\n            setError(true);\r\n            return false;\r\n        }\r\n\r\n        const username = JSON.parse(localStorage.getItem('user')).name;\r\n\r\n        Axios.put(`http://localhost:5000/api/update/${id}`, {prodName: prodName, price: price, username: username, category: category, company: company}).then((response) => {\r\n            //console.log(response);\r\n            navigate('/');\r\n        });\r\n    };\r\n\r\n    return(\r\n        <div className = 'product'>\r\n            <h1>Update Product</h1>\r\n            <input type = 'text' placeholder=\"Enter Product Name\" className=\"inputBox\" onChange={(e)=>{setProdName(e.target.value)}} value = {prodName}/>\r\n            {error &&  !prodName && <span className=\"invalid-input\">Enter valid name</span>}\r\n\r\n            <input type = 'text' placeholder=\"Enter Product Price\" className=\"inputBox\" onChange={(e)=>{setPrice(e.target.value)}} value = {price}/>\r\n            {error &&  !price && <span className=\"invalid-input\">Enter valid price</span>}\r\n\r\n            <input type = 'text' placeholder=\"Enter Product Category\" className=\"inputBox\" onChange={(e)=>{setCategory(e.target.value)}} value = {category}/>\r\n            {error &&  !category && <span className=\"invalid-input\">Enter valid category</span>}\r\n\r\n            <input type = 'text' placeholder=\"Enter Product Company\" className=\"inputBox\" onChange={(e)=>{setCompany(e.target.value)}} value = {company}/>\r\n            {error &&  !company && <span className=\"invalid-input\">Enter valid company</span>}\r\n\r\n            <button className=\"appButton\" onClick={() => {updateProduct(params.id)}}>Submit</button>\r\n        </div>\r\n    )\r\n};\r\n\r\nexport default UpdateComponent;",{"ruleId":"72","severity":1,"message":"73","line":4,"column":8,"nodeType":"74","messageId":"75","endLine":4,"endColumn":9},{"ruleId":"72","severity":1,"message":"76","line":5,"column":8,"nodeType":"74","messageId":"75","endLine":5,"endColumn":14},{"ruleId":"77","severity":1,"message":"78","line":15,"column":13,"nodeType":"79","endLine":15,"endColumn":52},{"ruleId":"80","severity":1,"message":"81","line":16,"column":7,"nodeType":"82","endLine":16,"endColumn":9,"suggestions":"83"},{"ruleId":"80","severity":1,"message":"81","line":16,"column":7,"nodeType":"82","endLine":16,"endColumn":9,"suggestions":"84"},{"ruleId":"80","severity":1,"message":"85","line":16,"column":7,"nodeType":"82","endLine":16,"endColumn":9,"suggestions":"86"},"no-unused-vars","'$' is defined but never used.","Identifier","unusedVar","'Popper' is defined but never used.","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'navigate'. Either include it or remove the dependency array.","ArrayExpression",["87"],["88"],"React Hook useEffect has a missing dependency: 'getProductDetails'. Either include it or remove the dependency array.",["89"],{"desc":"90","fix":"91"},{"desc":"90","fix":"92"},{"desc":"93","fix":"94"},"Update the dependencies array to be: [navigate]",{"range":"95","text":"96"},{"range":"97","